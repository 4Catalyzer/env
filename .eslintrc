{
  "plugins": ["prettier"],
  "extends": ["4catalyzer", "prettier","4catalyzer-typescript"],
  "env": {
      "node": true
  },
  "rules": {
    "prettier/prettier": "error",
    "global-require": "off",
    "import/no-dynamic-require": "off"
  },
  "overrides": [
    {
      "files": ["test/**"],
      "plugins": ["jest", "eslint-config-4catalyzer-jest"]
    }
  ]
}
